  function checkPrime(num){
  let count = 0;
  for(let i=i; i<=num; i++){
  if(num%i == 0){
   count++;
      }
    }
    if(count == 2){
       return true;
      }
       return false;
  }

   let ans == checkprime(11);

    if(ans == true){
    console.log("It is a prime");
  }
    else{
      console.log("It is not a prime");
   }