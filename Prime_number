  function checkPrime(number){
  let counter = 0;
  for(let i=i; i<=num; i++){
  if(number % i == 0){
   counter++;
      }
    }
    if(counter == 2){
       return true;
      }
       return false;
  }

   let answer = checkprime(11);

    if(answer == true){
    console.log("Prime");
  }
    else{
      console.log("Not a Prime");
   }